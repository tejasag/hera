let map = fn (arr, f) {
    let iter = fn (array, acc ) {
        if (len(array) == 0) {
            acc
        } else {
            let n = tail(array);
            let res = f(array[0]);
            iter(n, push(acc, res));
        };
    };
    iter(arr, [])
};

let first = fn (arr) { arr[0] };

let last = fn (arr) { arr[-1] };

let while = fn(condition, body) {
    if(condition()) {
        body();
        while(condition, body);
    } 
};
